{
  "name": "@shlappas/sorted-array",
  "version": "1.1.0",
  "description": "An array that keeps its elements in order",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrismilson/sorted-array.js.git"
  },
  "bugs": {
    "url": "https://github.com/chrismilson/sorted-array.js/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:docs": "typedoc --out docs src/index.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix": "yarn lint --fix",
    "prebuild": "yarn lint",
    "build": "rollup -c",
    "test": "jest",
    "prepare": "yarn build"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "keywords": [
    "sorted array",
    "weight balanced tree",
    "binary tree",
    "iterable"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.37.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@shlappas/itertools": "^2.1.1",
    "wbtree": "^0.0.0"
  }
}
